<Window x:Class="_06_01_AsyncAwait.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_06_01_AsyncAwait"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <ToolBar DockPanel.Dock="Top">
            <Button Click="Button_Click">Generate</Button>
        </ToolBar>
        <ProgressBar DockPanel.Dock="Bottom" Value="20" Height="15" IsIndeterminate="True"></ProgressBar>
        <ListBox x:Name="list"></ListBox>

    </DockPanel>
</Window>
